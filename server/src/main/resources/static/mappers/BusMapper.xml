<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="BusDAO">
    <select id="getAll" resultType="com.chacha.biz.dto.BusStationDTO">
        select * from BUS_STATION
    </select>
    <insert id="insertBusStation" parameterType="com.chacha.biz.dto.BusStationDTO">
        insert into BUS_STATION values(${STOP_NO},#{STOP_NM},${XCODE},${YCODE},${NODE_ID},#{STOP_TYPE})
    </insert>
</mapper>